<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qc.provider.mapper.UserLoginMapper">

    <resultMap type="com.qc.provider.entity..UserLoginEntity" id="userLoginMap">
		<result property="userId" column="user_id"/>
		<result property="loginName" column="login_name"/>
		<result property="password" column="password"/>
    </resultMap>

	<select id="selectbyName" parameterType="String" resultMap="userLoginMap">
		select * from user_login where login_name
	</select>

</mapper>